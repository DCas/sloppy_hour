<h1><%= @venue.name %></h1>
<p><%= @venue.address %><p>

<% if user_signed_in? %>
  <%= link_to 'Edit', edit_venue_path(@venue) %>
  <%= link_to 'Destroy', @venue, method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>

<%= link_to 'Add New Deal', new_deal_path(:venue_id => @venue.id), method: 'get' %>